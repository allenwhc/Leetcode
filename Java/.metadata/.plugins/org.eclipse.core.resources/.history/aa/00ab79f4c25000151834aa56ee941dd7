import java.util.Arrays;
import java.util.Stack;

public class Solution {
	public static boolean verifyPreorder(int[] nums){
		Stack<Integer> stack=new Stack<>();
		int n=nums.length;
		if(n==0) return true;
		stack.push(nums[0]);
		//First, find the leftmost leaf node
		for(int i=1; i<n; i++){
			if(nums[i]<stack.peek())
				stack.push(nums[i]);
			else break;
		}
		System.out.println(stack.peek());
		return false;
	}
	
	public static void main(String[] args){
		int[] nums={15,7,3,1,5,11,9,13,23,19,17,21,27,25,29};
		System.out.println(Arrays.toString(nums));
		if(verifyPreorder(nums))
			System.out.println(" is a preorder sequence of a BST.");
		else
			System.out.println(" is not a preorder sequence of a BST.");
	}
}
