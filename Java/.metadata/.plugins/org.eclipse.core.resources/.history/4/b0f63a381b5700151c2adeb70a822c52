import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class Relation {
	private static List<List<Character>> adjacencyList=new ArrayList<List<Character>>();
	public Relation(Character[][] matrix){
		for(Character[] cc:matrix)
			adjacencyList.add(Arrays.asList(cc));
	}
	
