import java.util.ArrayList;
import java.util.Arrays;
import java.util.HashMap;
import java.util.List;

public class Solution {
	//Strobogrammatic Number I
	public static boolean isStrobogrammatic(String num){
		int len=num.length();
		if(len==0) return true;
		if(num.charAt(0)=='-') return false; //eliminate all negative numbers
		HashMap<Character, List<Integer>> hmap=new HashMap<>();
		for(int i=0; i<len; i++){
			char c=num.charAt(i);
			if(!hmap.containsKey(c)){
				List<Integer> l=new ArrayList<>();
				l.add(i);
				hmap.put(c, l);
			}
			else
				hmap.get(c).add(i);
		}
		
//		for(char key:hmap.keySet())
//			System.out.println(key+": "+Arrays.deepToString(hmap.get(key).toArray()));
		for(char key:hmap.keySet()){
			if(key=='2' || key=='3' || key=='4' || key=='5' || key=='7') return false;	//2,3,4,5,7 can not appear in strobo
			else if(key=='6'){
				if(!hmap.containsKey('9') || hmap.get(key).size()!=hmap.get('9').size()) return false;
				
			}
		}
		return true;
	}
		
	//Main Method
	public static void main(String[] args){
		String num="9116";
		if(isStrobogrammatic(num))
			System.out.println(num+" is a strobogrammatic number.");
		else
			System.out.println(num+" is not a strobogrammatic number.");
	}
}
